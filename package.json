  "config": {
    "ionic_src_dir": "src/mobile"
  },
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "start:raw": "npm run server:raw",
    "start:hmr": "npm run server -- --hot",
    "start": "npm run server",
    "ionic:prod": "npm run ionic-serve:prod",
    "ionic": "npm run ionic-serve",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server:raw": "webpack-dev-server --inline --port 8080 --host 0.0.0.0",
    "server": "webpack-dev-server --inline --port 8080 --host 0.0.0.0",
    "prebuild": "npm run browser-tsconfig",
    "preserver": "npm run browser-tsconfig",
    "preserver:raw": "npm run browser-tsconfig",
    "browser-tsconfig": "gulp tsconfig",
    "ionic-tsconfig": "gulp tsconfig --mobile",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/browser/app/app.component.ts",
    "postinstall": "npm run webdriver-update",

    "ionic-compress": "ionic-app-scripts compress",
    "ionic-copy": "ionic-app-scripts copy",
    "ionic-watch": "ionic-app-scripts watch",

    "ionic-serve:prod": "ionic serve",
    "preionic-serve:prod": "npm run ionic-build:prod",
    "ionic-build:prod": "ionic-app-scripts build",
    "preionic-build:prod": "npm run ionic-tsconfig",

    "ionic-serve": "ionic serve",
    "preionic-serve": "npm run ionic-build",
    "ionic-build": "ionic-app-scripts build --dev",
    "preionic-build": "npm run ionic-tsconfig",
    
    "preemulate": "npm run ionic-build:prod",
    "predeploy": "npm run ionic-build:prod",
    "prerun": "npm run ionic-build:prod"
  }
